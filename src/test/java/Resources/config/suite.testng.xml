<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Pool" parallel="tests">
    <test name="PoolTestChrome" thread-count="4" parallel="tests">
        <parameter name="config" value="suite.conf.json"/>
        <parameter name="environment" value="chrome"/>
        <classes>
            <class name="Specs.ORGTestSuite"/>

        </classes>
    </test>

    <test name="PoolTestFirefox" thread-count="4" parallel="classes">
        <parameter name="config" value="suite.conf.json"/>
        <parameter name="environment" value="firefox"/>
        <classes>
            <class name="Specs.ShopTestSuite"/>

        </classes>
    </test>
</suite>
