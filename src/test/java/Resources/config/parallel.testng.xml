<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Parallel" thread-count="3" parallel="methods">
    <test name="SingleTestChrome">
        <parameter name="conf" value="parallel.conf.json"/>
        <parameter name="environment" value="chrome"/>
        <classes>
            <class name="Specs.ORGTestSuite"/>
        </classes>
    </test>

    <test name="SingleTestFirefox">
        <parameter name="conf" value="parallel.conf.json"/>
        <parameter name="environment" value="firefox"/>
        <classes>
            <class name="Specs.ORGTestSuite"/>
        </classes>
    </test>

    <test name="SingleTestSafari">
        <parameter name="conf" value="parallel.conf.json"/>
        <parameter name="environment" value="safari"/>
        <classes>
            <class name="Specs.ORGTestSuite"/>
        </classes>
    </test>
</suite>
